<img src="/images/member.png" />

<div class="info">
    <div class="heading">
        <strong>{{first_name}} {{last_name}}</strong>
        <span class="info"></span>
    </div>

    <div class="detail">
        <span class="email"><a href="mailto:{{email}}">{{email}}</a></span>
        <span class="phone">{{tel}}</span>
    </div>
</div>

<ul>
    <li><a class="open" href="#members/{{_id}}">open</a></li>
</ul>